
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col s8 offset-s2 m6 offset-m3">
            <div class="card center-align">
                <div class="card-image">
                    <img class="activator" src="~/images/licenciasnew.jpg">
                </div>
                <div class="card-tabs">
                    <ul class="tabs tabs-fixed-width">
                        <li class="tab"><a href="#test4">Ingresar</a></li>
                    </ul>
                </div>
                <div class="card-content grey lighten-4">
                    <div id="test4">
                        <div class="row">
                            <form class="col s12" asp-controller="Status" asp-action="Search" method="POST">
                                <div class="input-field col s12">
                                    <i class="material-icons prefix">domain</i>
                                    <input placeholder="1470848271" id="Ticket" name="Ticket" type="text" class="validate">
                                    <label for="email">N° Seguimiento</label>
                                </div>
                                <div>
                                    <button type="submit" name="action" id="action" class="waves-effect waves-light btn red"><i class="material-icons right">arrow_forward</i>Ingresar</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

